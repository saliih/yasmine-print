{% extends 'SonataAdminBundle:CRUD:edit.html.twig' %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        $(document).ready(function () {

            $inputs = $('input[id$=_img]');
            $.each($inputs, function (indec, element) {
                var id = $(element).attr('id');
                var html = htmlbt(id);
                $(element).after(html);


            });
        });
    </script>
{% endblock %}