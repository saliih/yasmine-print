{% extends 'SonataAdminBundle:CRUD:edit.html.twig' %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        $(document).ready(function () {

            var $inputs = $('input[id$=_bg]');
            $.each($inputs, function (indec, element) {
                var id = $(element).attr('id');
                var html = htmlbt(id);
                $(element).after(html);
            });
            var $inputs = $('input[id$=_pdf]');
            $.each($inputs, function (indec, element) {
                var id = $(element).attr('id');
                var html = htmlbt(id);
                $(element).after(html);
            });
        });
    </script>
{% endblock %}